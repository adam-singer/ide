{
  "manifest_version": 2,
  "name": "Dart chrome editor",
  "version": "1",
  "minimum_chrome_version": "23",
  "icons": {"128": "dart_icon.png"},
  "app": {
    "launch": {
       "local_path": "dart_chrome_editor.html"
    }
  },
  "permissions": ["experimental","https://apis.google.com/*","https://www.googleapis.com/*"],
  "oauth2": {
    "client_id": "897507319316-0j3p4c925ahn7tsej9d8br0gde6vnui0.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/userinfo.profile","https://www.googleapis.com/auth/drive"]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}